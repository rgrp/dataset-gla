{
  "name": "os-gb-local-gla",
  "title": "Greater London Authority (GLA) Spending",
  "licenses": [
    {
      "url": "http://opendatacommons.org/licenses/pddl/1.0/", 
      "name": "Public Domain Dedication and License"
    }
  ], 
  "sources": [{
    "web": "http://www.london.gov.uk/mayor-assembly/gla/spending-money-wisely/budget-expenditure-charges/expenditure-over-250"
  }],
  "resources": [
    {
      "path": "data/all.csv",
      "schema": {
        "fields": [
          {
            "type": "integer", 
            "primarykey": true,
            "id": "ID"
          }, 
          {
            "type": "integer", 
            "id": "VendorID"
          }, 
          {
            "type": "string", 
            "id": "Vendor"
          }, 
          {
            "type": "string", 
            "id": "AccountCode"
          }, 
          {
            "type": "string", 
            "id": "AccountDescription"
          }, 
          {
            "type": "integer", 
            "id": "DocNo"
          }, 
          {
            "type": "number", 
            "id": "Amount"
          }, 
          {
            "type": "date", 
            "id": "Date"
          },
          {
            "type": "string", 
            "description": "Source file and line where this data came from in form {line-no}-{filename-minus-.csv}",
            "id": "Source"
          }
        ]
      }
    }
  ]
}
